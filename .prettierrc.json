{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "avoid",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "htmlWhitespaceSensitivity": "ignore",
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-organize-imports"]
}
